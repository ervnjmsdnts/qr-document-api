generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  ADMIN
  CHECKER
  CLERK
}

enum UserStatus {
  ENABLED
  DISABLED
}

model User {
  id       String     @unique
  name     String
  email    String     @unique
  role     UserRole
  isActive Boolean    @default(true)
  Status   UserStatus @default(ENABLED)
}
